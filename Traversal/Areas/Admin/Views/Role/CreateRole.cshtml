@model CreateRoleViewModel
@{
    ViewData["Title"] = "CreateRole";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Yeni Rol Oluşturma Sayfası</h1>

<form method="post">

    <label>Role Adı</label>
    <input type="text" asp-for="RoleName" class="form-control" />
    <br />

    <button class="btn btn-outline-primary">Yeni Rolü Ekle</button>

</form>
